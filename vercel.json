{
  "version": 2,
  
  "startCommand": "npm run start",
  "outputDirectory": "frontend/frontend/dist",
  "framework": null,
  "builds": [
    {
      "src": "backend/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/frontend/dist/index.html"
    }
  ]
}
